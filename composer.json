    "type": "neos-package",
    "name": "wwwision/renderlets-provider",
    "license": "MIT",
    "authors": [
        {
            "name": "bwaidelich",
            "email": "b.waidelich@wwwision.de"
        }
    ],
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/bwaidelich"
